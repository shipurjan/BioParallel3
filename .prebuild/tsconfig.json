{
  "compilerOptions": {
    "lib": ["ES2020"],
    "target": "ES2020",
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@scripts/*": ["scripts/*"],
      "@/*": ["./*"],
      "@bioparallel/*": ["../*"]
    }
  }
}
